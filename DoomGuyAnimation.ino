void msg_time(int x, int y, String msg, uint32_t t)
{
    t = millis() - t;
    tft.setCursor(x, y);
    tft.print(msg);
    tft.print(t);
    tft.print(F("ms"));
}

//The setup function is called once at startup of the sketch
void setup()
{
	 uint16_t ID = tft.readID();
	 tft.begin(ID);
	 tft.setRotation(3);
}


void drawImg(int x,int y,int w,int h,unsigned short *bmp,int alpha,bool flip)
{
	int size = 5;
	int pos = 0;

	for(int i=0; i<w;i++)
	{
		for(int j=0; j<h;j++)
		{
			if(flip) pos = i+(j*w);
			else pos = (w-i)+(j*w);
			int col = pgm_read_word_near(bmp + pos);
			if(col == ALPHA_COL) col = BLACK;


			if(flip)
			{

				tft.fillRect(x + (i*size), y + (j*size), size, size, col);
			}
			else
			{
				tft.fillRect(x + (i*size), y + (j*size), size, size, col);
			}

		}
	}
}

